{
  "name": "babel-plugin-auto-css-modules",
  "version": "0.0.5",
  "description": "Automatically import same name CSS modules.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "nodely -s ./src -o ./lib",
    "postpublish": "codecov",
    "prepublishOnly": "npm run build",
    "test": "lintly src && flow && npm run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dogma-io/vmdom.git"
  },
  "keywords": [
    "auto",
    "automatic",
    "babel",
    "css",
    "import",
    "module",
    "modules",
    "plugin",
    "require",
    "webpack"
  ],
  "author": "Matthew Dahl <https://github.com/dogma-io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dogma-io/vmdom/issues"
  },
  "homepage": "https://github.com/dogma-io/vmdom#readme",
  "devDependencies": {
    "babel-core": "^7.0.0-0",
    "babel-jest": "22.4.1",
    "codecov": "3.0.0",
    "jest-cli": "22.4.2",
    "lintly": "0.0.25",
    "nodely": "0.0.29"
  },

  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/**/__tests__/**/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testRegex": "/__tests__/.*-test\\.js$"
  }
}
